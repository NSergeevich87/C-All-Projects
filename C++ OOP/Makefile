.PHONY: clean All

All:
	@echo "----------Building project:[ SharedPointers - Debug ]----------"
	@cd "SharedPointers" && "$(MAKE)" -f  "SharedPointers.mk"
clean:
	@echo "----------Cleaning project:[ SharedPointers - Debug ]----------"
	@cd "SharedPointers" && "$(MAKE)" -f  "SharedPointers.mk" clean
