.PHONY: clean All

All:
	@echo "----------Building project:[ PointerArithmetic - Debug ]----------"
	@cd "PointerArithmetic" && "$(MAKE)" -f  "PointerArithmetic.mk"
clean:
	@echo "----------Cleaning project:[ PointerArithmetic - Debug ]----------"
	@cd "PointerArithmetic" && "$(MAKE)" -f  "PointerArithmetic.mk" clean
